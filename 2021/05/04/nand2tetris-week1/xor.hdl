/**
 *  Exclusive-or gate: out = !(a == b).
 */

CHIP xor {

    IN  a, b;
    OUT out;

    PARTS:
    Not(in=a, out=nota);
    Not(in=b, out=notb);
    And(a=a, b=notb, out=andanotb);
    And(a=nota, b=b, out=andnotab);
    Or(a=andanotb, b=andnotab, out=out);
}